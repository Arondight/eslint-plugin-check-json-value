{
  "plugins": ["check-json-value"],
  "rules": {
    "check-json-value/json-value": [
      "error",
      [
        {
          "file": "test_json_value\\.json",
          "path": "data.records[0].id",
          "values": [
            {
              "type": "number",
              "value": "^\\d{4,6}$",
              "ignoreCase": false
            },
            {
              "type": "null",
              "value": null,
              "ignoreCase": false
            }
          ]
        }
      ]
    ]
  }
}
