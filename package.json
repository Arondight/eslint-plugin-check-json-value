{
  "name": "eslint-plugin-check-json-value",
  "version": "1.0.0",
  "description": "ESLint plugin to check value of JSON file",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Qin Fandong",
  "main": "lib/index.js",
  "scripts": {
    "test": "npx mocha tests --recursive",
    "check": "npx eslint ./.*.json ./lib/ ./tests/",
    "format": "npx prettier --config ./.prettierrc.json -uw ./.*.json ./*.md ./lib/ ./tests/"
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-plugin-eslint-plugin": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.3",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "MIT"
}
